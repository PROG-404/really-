{
  "Themes": {
    "Default": {
      "Primary": {
        "Main": "#19191E",
        "Secondary": "#1E1E23",
        "Accent": "#3C91FF",
        "AccentDark": "#287DEB",
        "Text": "#FFFFFF",
        "SubText": "#C8C8C8",
        "Border": "#323237",
        "Background": "#141419",
        "Success": "#2DC85F",
        "Warning": "#FAB428",
        "Error": "#FA3C3C"
      },
      "Dark": {
        "Main": "#141419",
        "Secondary": "#19191E",
        "Accent": "#3287F5",
        "AccentDark": "#1E73E1",
        "Text": "#FFFFFF",
        "SubText": "#BEBEBE",
        "Border": "#2D2D32",
        "Background": "#0F0F14"
      },
      "Light": {
        "Main": "#F5F5FA",
        "Secondary": "#F0F0F5",
        "Accent": "#3287F5",
        "AccentDark": "#1E73E1",
        "Text": "#1E1E23",
        "SubText": "#64646E",
        "Border": "#DCDCE1",
        "Background": "#FAFAFF"
      },
      "Custom": {
        "Main": "#333333",
        "Secondary": "#444444",
        "Accent": "#FF6600",
        "Text": "#FFFFFF",
        "Background": "#000000"
      }
    }
  },
  "Animations": {
    "Effects": {
      "Ripple": {
        "Duration": 0.5,
        "EasingStyle": "Quart",
        "Transparency": 1
      },
      "Spring": {
        "Speed": 20,
        "Damping": 0.8
      },
      "Fade": {
        "Duration": 0.3,
        "EasingStyle": "Quad"
      },
      "Scale": {
        "Duration": 0.3,
        "EasingStyle": "Back"
      },
      "SlideIn": {
        "Duration": 0.6,
        "Direction": "Left",
        "EasingStyle": "Back"
      },
      "Rotate": {
        "Duration": 1.0,
        "Angle": 360,
        "EasingStyle": "Linear"
      }
    }
  },
  "UIComponents": {
    "Button": {
      "DefaultSize": {"ScaleX": 1, "OffsetX": 0, "ScaleY": 0, "OffsetY": 40},
      "CornerRadius": 6,
      "StrokeTransparency": 0.5,
      "HoverEffect": true,
      "ClickEffect": true
    },
    "Window": {
      "DefaultSize": {"Width": 600, "Height": 400},
      "CornerRadius": 8,
      "Draggable": true,
      "CloseButtonColor": "#FA3C3C"
    },
    "Slider": {
      "MinValue": 0,
      "MaxValue": 100,
      "DefaultValue": 50,
      "TrackColor": "#323237",
      "HandleColor": "#3C91FF"
    }
  },
  "Notifications": {
    "Default": {
      "Duration": 3,
      "BackgroundColor": "#1E1E23",
      "TextColor": "#FFFFFF",
      "CornerRadius": 8
    },
    "Warning": {
      "Duration": 5,
      "BackgroundColor": "#FAB428",
      "TextColor": "#000000"
    },
    "Error": {
      "Duration": 4,
      "BackgroundColor": "#FA3C3C",
      "TextColor": "#FFFFFF"
    }
  },
  "Performance": {
    "FPSLimit": 60,
    "EnableOptimization": true,
    "StreamingEnabled": true
  },
  "Audio": {
    "DefaultVolume": 1.0,
    "MuteOnFocusLost": true,
    "Effects": {
      "Reverb": true,
      "Echo": false
    }
  }
}
